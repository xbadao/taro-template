{
  "extends": ["taro"],
  "rules": {
    "no-unused-vars": ["error", { "varsIgnorePattern": "Taro" }],
    "jsx-quotes": ["error", "prefer-double"],
    "react/jsx-filename-extension": [
      1,
      { "extensions": [".js", ".jsx", ".tsx"] }
    ],
    "react-hooks/rules-of-hooks": 0
  },
  "settings": {
    "react": {
      "version": "detect"
    }
  },
  "parser": "babel-eslint"
}
